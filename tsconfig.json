{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"isolatedModules": true,
		"jsx": "react-native",
		"lib": ["ES6", "ES2021"],
		"moduleResolution": "Node",
		"noEmit": true,
		"target": "ESNext"
	}
}
